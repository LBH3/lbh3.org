{
  "ci": {
    "assert": {
      "preset": "lighthouse:all",
      "assertions": {
        "bootup-time": "warn",
        "categories:accessibility": ["error", {"minScore": 0.95}],
        "categories:best-practices": ["error", {"minScore": 0.90}],
        "categories:performance": ["error", {"minScore": 0.70}],
        "categories:pwa": ["error", {"minScore": 0.48}],
        "categories:seo": ["error", {"minScore": 1}],
        "color-contrast": "warn",
        "critical-request-chains": "warn",
        "estimated-input-latency": "warn",
        "first-contentful-paint": "warn",
        "first-cpu-idle": "warn",
        "first-meaningful-paint": "warn",
        "font-display": "warn",
        "image-alt": "warn",
        "image-aspect-ratio": "warn",
        "interactive": "warn",
        "load-fast-enough-for-pwa": "warn",
        "mainthread-work-breakdown": "warn",
        "max-potential-fid": "warn",
        "offline-start-url": "warn",
        "render-blocking-resources": "warn",
        "service-worker": "warn",
        "speed-index": "warn",
        "tap-targets": "warn",
        "time-to-first-byte": "warn",
        "unminified-css": "warn",
        "unused-css-rules": "warn",
        "uses-http2": "warn",
        "uses-long-cache-ttl": "warn",
        "uses-passive-event-listeners": "warn",
        "uses-rel-preload": "warn",
        "uses-text-compression": "warn",
        "uses-webp-images": "warn",
        "works-offline": "warn"
      }
    }
  }
}
