{
  "name": "lbh3",
  "version": "0.0.1",
  "description": "LBH3 website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBH3/lbh3.org.git"
  },
  "bugs": {
    "url": "https://github.com/LBH3/lbh3.org/issues"
  },
  "homepage": "https://github.com/LBH3/lbh3.org#readme",
  "private": true,
  "main": "src",
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "cd public && rm -rf ./dist && node build && cd ..",
    "build:tests": "cd public && node build-tests && cd ..",
    "build:tests:webpack": "cd public && npm run build:tests:production:webpack && cd ..",
    "build:webpack": "cd public && npm run build:webpack:production && cd ..",
    "build:webpack:watch": "cd public && npm run build:webpack:watch && cd ..",
    "bundlesize": "node ./node_modules/.bin/bundlesize",
    "bundlesize:webpack": "node ./node_modules/.bin/bundlesize --config config/bundlesize.webpack.json",
    "ci": "node install-ci",
    "eslint": "eslint src/. test/. --config .eslintrc.json --fix",
    "heroku-postbuild": "npm run build:webpack",
    "install": "node install",
    "mocha": "mocha test/ --recursive --exit",
    "mocha:debug": "mocha test/ --recursive --inspect-brk --exit",
    "sequelize:migrate": "npx sequelize-cli db:migrate",
    "sequelize:migrate:rerun": "npm run sequelize:migrate:undo && npm run sequelize:migrate",
    "sequelize:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "start": "node --optimize_for_size --max_old_space_size=460 src/",
    "start:debug": "node --inspect src/",
    "start:debug:webpack": "node --inspect src/ -- --webpack & sleep 1 && npm run build:webpack:watch",
    "start:development": "node src/ --develop",
    "start:inspect": "node --inspect-brk src/",
    "start:production": "NODE_ENV=production PORT=8080 npm start",
    "test": "npm run test:common && npm run testee && npm run testee:webpack",
    "test:ci": "npm run test:common && node public/test.sauce-labs.js",
    "test:common": "npm run eslint && npm run build && npm run build:webpack && npm run bundlesize && npm run bundlesize:webpack && npm run test:db && npm run mocha && npm run build:tests && npm run build:tests:webpack",
    "test:db": "npm run test:db:create && npm run sequelize:migrate",
    "test:db:create": "psql -c 'drop database if exists lbh3_test;' -h localhost -U postgres && psql -c 'create database lbh3_test;' -h localhost -U postgres && psql lbh3_test < ./test/lbh3_test.dump -h localhost -U postgres -v ON_ERROR_STOP=1",
    "test:production": "npm run test:common && npm run testee:production && npm run testee:production:webpack",
    "testee": "./public/node_modules/.bin/testee public/test.html --browsers firefox --reporter Spec",
    "testee:production": "./public/node_modules/.bin/testee public/test.production.html --browsers firefox --reporter Spec",
    "testee:production:webpack": "./public/node_modules/.bin/testee public/test.production.webpack.html --browsers firefox --reporter Spec",
    "testee:webpack": "./public/node_modules/.bin/testee public/test.webpack.html --browsers firefox --reporter Spec"
  },
