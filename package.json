{
  "name": "lbh3",
  "version": "0.0.1",
  "description": "LBH3 website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBH3/lbh3.org.git"
  },
  "bugs": {
    "url": "https://github.com/LBH3/lbh3.org/issues"
  },
  "homepage": "https://github.com/LBH3/lbh3.org#readme",
  "private": true,
  "main": "src",
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "build": "cd public && node build",
    "db-migrate:create": "db-migrate create",
    "db-migrate:down": "db-migrate down",
    "db-migrate:rerun": "npm run db-migrate:down && npm run db-migrate:up",
    "db-migrate:reset": "db-migrate reset",
    "db-migrate:up": "db-migrate up",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "heroku-postbuild": "npm run build",
    "install": "node install",
    "mocha": "mocha test/ --recursive",
    "start": "node src/",
    "start:development": "node src/ --develop",
    "start:production": "NODE_ENV=production PORT=8080 npm start",
    "test": "npm run eslint && npm run mocha && npm run testee",
    "testee": "./public/node_modules/.bin/testee public/test.html --browsers firefox --reporter Spec"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.2.4",
    "done-ssr-middleware": "^1.0.2",
    "feathers": "^2.1.4",
    "feathers-authentication": "^1.2.6",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-authentication-jwt": "^0.3.1",
    "feathers-authentication-oauth2": "^0.2.5",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.8.1",
    "feathers-hooks": "^2.0.1",
    "feathers-hooks-common": "^3.5.5",
    "feathers-rest": "^1.7.4",
    "feathers-sequelize": "^2.0.1",
    "feathers-socketio": "^2.0.0",
    "helmet": "^3.6.1",
    "passport-facebook": "^2.1.1",
    "pg": "^6.4.0",
    "sequelize": "^4.2.1",
    "serve-favicon": "^2.4.3",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "mocha": "^3.4.2",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  }
}
