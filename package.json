{
  "name": "lbh3",
  "version": "0.0.1",
  "description": "LBH3 website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBH3/lbh3.org.git"
  },
  "bugs": {
    "url": "https://github.com/LBH3/lbh3.org/issues"
  },
  "homepage": "https://github.com/LBH3/lbh3.org#readme",
  "private": true,
  "main": "src",
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "10.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "cd public && node build && cd ..",
    "build:tests": "cd public && node build-tests && cd ..",
    "bundlesize": "node ./node_modules/.bin/bundlesize",
    "ci": "node install-ci",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "heroku-postbuild": "npm run build",
    "install": "node install",
    "mocha": "mocha test/ --recursive --exit",
    "mocha:debug": "mocha test/ --recursive --inspect-brk --exit",
    "sequelize:migrate": "sequelize db:migrate",
    "sequelize:migrate:rerun": "npm run sequelize:migrate:undo && npm run sequelize:migrate",
    "sequelize:migrate:undo": "sequelize db:migrate:undo",
    "start": "node --optimize_for_size --max_old_space_size=460 src/",
    "start:ci": "npm run test:db:create && http-server -p 3000 --silent",
    "start:debug": "node --inspect src/",
    "start:development": "node src/ --develop",
    "start:production": "NODE_ENV=production PORT=8080 npm start",
    "test": "npm run eslint && npm run build && bundlesize && npm run mocha && npm run testee",
    "test:ci": "npm run test:production && node public/test.sauce-labs.js",
    "test:db:create": "psql -c 'drop database if exists lbh3_test;' -U postgres && psql -c 'create database lbh3_test;' -U postgres && psql lbh3_test < ./test/lbh3_test.dump -U postgres -v ON_ERROR_STOP=1",
    "test:production": "npm run eslint && npm run build && bundlesize && npm run build:tests && npm run mocha && npm run testee:production",
    "testee": "./public/node_modules/.bin/testee public/test.html --browsers firefox --reporter Spec",
    "testee:production": "./public/node_modules/.bin/testee public/test.production.html --browsers firefox --reporter Spec"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.6",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-oauth2": "^1.0.3",
    "@feathersjs/configuration": "^4.3.4",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/feathers": "^3.1.5",
    "@feathersjs/socketio": "^3.2.1",
    "aws-sdk": "^2.99.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "done-ssr": "^3.2.7",
    "done-ssr-middleware": "^3.0.0",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-hooks-common": "^3.7.1",
    "feathers-sequelize": "^2.4.0",
    "googleapis": "^39.2.0",
    "helmet": "^3.8.1",
    "honeybadger": "^1.3.0",
    "html-entities": "^1.2.1",
    "json2csv": "^4.1.3",
    "marked": "^0.6.2",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "raygun": "^0.10.0",
    "sequelize": "^4.31.2",
    "sequelize-cli": "^4.0.0",
    "serve-favicon": "^2.4.3",
    "striptags": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "bundlesize": "^0.17.1",
    "eslint": "^4.4.1",
    "http-server": "^0.11.1",
    "leakage": "^0.4.0",
    "mocha": "^5.1.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "through": "^2.3.8",
    "xml-stream": "^0.4.5"
  },
  "bundlesize": [
    {
      "path": "./public/dist/steal.production.js",
      "maxSize": "25 kB"
    },
    {
      "path": "./public/dist/bundles/edit-add-edit.css",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/dist/bundles/edit-add-edit.js",
      "maxSize": "29 kB"
    },
    {
      "path": "./public/dist/bundles/hareline-hasher--*.css",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/dist/bundles/hareline-hasher--*.js",
      "maxSize": "42 kB"
    },
    {
