{
  "name": "lbh3",
  "version": "0.0.1",
  "description": "LBH3 website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBH3/lbh3.org.git"
  },
  "bugs": {
    "url": "https://github.com/LBH3/lbh3.org/issues"
  },
  "homepage": "https://github.com/LBH3/lbh3.org#readme",
  "private": true,
  "main": "src",
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": "8.x",
    "npm": "5.7.x"
  },
  "scripts": {
    "build": "cd public && node build && cd ..",
    "ci": "node install-ci",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "heroku-postbuild": "npm run build",
    "install": "node install",
    "mocha": "mocha test/ --recursive",
    "sequelize:migrate": "sequelize db:migrate",
    "sequelize:migrate:rerun": "npm run sequelize:migrate:undo && npm run sequelize:migrate",
    "sequelize:migrate:undo": "sequelize db:migrate:undo",
    "start": "node --optimize_for_size --max_old_space_size=460 src/",
    "start:ci": "http-server -p 3000 --silent",
    "start:debug": "node --inspect src/",
    "start:development": "node src/ --develop",
    "start:production": "NODE_ENV=production PORT=8080 npm start",
    "test": "npm run eslint && npm run build && npm run mocha && npm run testee",
    "test:ci": "npm run test && node public/test.sauce-labs.js",
    "testee": "./public/node_modules/.bin/testee public/test.html --browsers firefox --reporter Spec"
  },
  "dependencies": {
    "aws-sdk": "^2.99.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "done-ssr": "^2.1.0",
    "done-ssr-middleware": "^2.0.0",
    "feathers": "^2.1.7",
    "feathers-authentication": "^1.2.7",
    "feathers-authentication-hooks": "^0.1.4",
    "feathers-authentication-jwt": "^0.3.2",
    "feathers-authentication-oauth2": "^0.3.0",
    "feathers-configuration": "^0.4.1",
    "feathers-errors": "^2.9.1",
    "feathers-hooks": "^2.0.2",
    "feathers-hooks-common": "^3.7.1",
    "feathers-sequelize": "^2.4.0",
    "feathers-socketio": "^2.0.0",
    "googleapis": "^21.2.0",
    "helmet": "^3.8.1",
    "html-entities": "^1.2.1",
    "marked": "^0.3.6",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "sequelize": "^4.31.2",
    "serve-favicon": "^2.4.3",
    "striptags": "^3.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "http-server": "^0.10.0",
    "mocha": "^3.5.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "through": "^2.3.8",
    "xml-stream": "^0.4.5"
  }
}
