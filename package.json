{
  "name": "lbh3",
  "version": "0.0.1",
  "description": "LBH3 website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LBH3/lbh3.org.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LBH3/lbh3.org/issues"
  },
  "homepage": "https://github.com/LBH3/lbh3.org#readme",
  "private": true,
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "node server --port 8080",
    "heroku-postbuild": "npm run build",
    "develop": "node server --develop --port 8080",
    "build": "node build"
  },
  "main": "lbh3/index.stache!done-autorender",
  "engines": {
    "node": "8.x"
  },
  "files": [
    "src"
  ],
  "steal": {
    "main": "lbh3/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "ext": {
      "html": "steal-stache"
    },
    "configDependencies": [
      "live-reload",
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "can-component": "^3.0.7",
    "can-connect": "^1.3.8",
    "can-define": "^1.0.17",
    "can-route": "^3.0.8",
    "can-route-pushstate": "^3.0.3",
    "can-set": "^1.1.0",
    "can-stache": "^3.0.20",
    "can-view-autorender": "^3.0.4",
    "can-zone": "^0.6.6",
    "done-autorender": "^1.0.0",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.0.0",
    "font-awesome": "^4.7.0",
    "generator-donejs": "^1.0.0",
    "steal": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.0.5",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0",
    "can-fixture": "^1.0.13"
  }
}
