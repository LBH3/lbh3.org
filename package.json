      "maxSize": "12 kB"
    },
    {
      "path": "./public/dist/bundles/lbh3/components/hashers/hashers.css",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/dist/bundles/lbh3/components/hashers/hashers.js",
      "maxSize": "7 kB"
    },
    {
      "path": "./public/dist/bundles/lbh3/components/home/home.css",
      "maxSize": "1 kB"
    },
    {
      "path": "./public/dist/bundles/lbh3/components/home/home.js",
      "maxSize": "7 kB"
    },
    {
      "path": "./public/dist/bundles/lbh3/components/past-runs/past-runs.css",
