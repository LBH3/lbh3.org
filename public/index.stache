<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, viewport-fit=cover">

    <title>{{title}}</title>
    <meta name="description" content="{{description}}">

    {{! Apple }}
    <link rel="apple-touch-icon" href="{{joinBase 'assets/apple-touch-icon-180x180.png'}}" sizes="180x180">
    <link rel="mask-icon" href="{{joinBase 'assets/safari-pinned-tab.svg'}}" color="#dc3232">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-title" content="LBH3">

    {{! Facebook }}
    <meta property="fb:app_id" content="857733227744669">

    {{! Microsoft }}
    <meta name="msapplication-TileColor" content="#dc3232">
    <meta name="msapplication-config" content="{{joinBase 'assets/browserconfig.xml'}}">

    {{! Open Graph }}
    <meta property="og:image" content="https://www.lbh3.org/dist/assets/fb-image.png">
    <meta property="og:title" content="{{ogTitle}}">

    {{! Generic app metadata }}
    <link rel="manifest" href="{{joinBase 'assets/manifest.json'}}">
    <meta name="application-name" content="LBH3">
    <meta name="theme-color" content="#dc3232">

    {{! Global Site Tag (gtag.js) - Google Analytics }}
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-91420608-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments)};
      gtag('js', new Date());
      gtag('config', 'UA-91420608-1');
    </script>
  </head>
  <body>
    <can-import from="~/styles.less" />
    <can-import from="~/app" export-as="viewModel" />
    <can-import from="~/components/footer/" />
    <can-import from="~/components/header/" />
    <can-import from="~/components/nav/" />

    <header>
      <lbh3-header
        id:from="id"
        page:from="page"
        secondaryPage:from="secondaryPage"
      />
      <lbh3-nav day:from="day" month:from="month" page:from="page" year:from="year" />
    </header>

    <main class="container-fluid {{page}}">
      {{#switch page}}
        {{#case "about"}}
          <can-import from="~/components/about/nav/">
            {{#if isPending}}
              Loading the About navigation…
            {{else}}
              <lbh3-about-nav
                class="container pl-0 pr-0"
                secondaryPage:from="secondaryPage"
              />
            {{/if}}
          </can-import>
          {{#switch secondaryPage}}
            {{#case "mismanagement"}}
              <can-import from="~/components/about/mismanagement/">
                {{#if isPending}}
                  Loading Mismanagement…
                {{else}}
                  <lbh3-about-mismanagement
                    class="container pl-0 pr-0"
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    title:to="scope.vm.title"
                    year:from="scope.vm.year"
                  />
                {{/if}}
              </can-import>
            {{/case}}
            {{#case "privacy"}}
              <can-import from="~/components/about/privacy/">
                {{#if isPending}}
                  Loading page…
                {{else}}
                  <lbh3-about-privacy
                    class="container pl-0 pr-0"
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    title:to="scope.vm.title"
                  />
                {{/if}}
              </can-import>
            {{/case}}
            {{#default}}
              <can-import from="~/components/about/">
                {{#if isPending}}
                  Loading about page…
                {{else}}
                  <lbh3-about
                    class="container pl-0 pr-0"
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    secondaryPage:from="secondaryPage"
                    title:to="scope.vm.title"
                  />
                {{/if}}
              </can-import>
            {{/default}}
          {{/switch}}
        {{/case}}
        {{#case "erections"}}
          {{#if urlId}}
            {{#if secondaryPage}}
              {{#switch secondaryPage}}
                {{#case "ballots"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/erection/ballots/">
                      {{#if isPending}}
                        Loading the erection ballots page…
                      {{else}}
                        <lbh3-erection-ballots
                          class="container pl-0 pr-0"
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          secondaryPage:from="scope.vm.secondaryPage"
                          title:to="scope.vm.title"
                          urlId:from="scope.vm.urlId"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "edit"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/erection/edit/">
                      {{#if isPending}}
                        Loading the edit erection page…
                      {{else}}
                        <lbh3-erection-edit
                          class="container pl-0 pr-0"
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          secondaryPage:from="scope.vm.secondaryPage"
                          title:to="scope.vm.title"
                          urlId:from="scope.vm.urlId"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "eligibility"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/erection/eligibility/">
                      {{#if isPending}}
                        Loading the erection eligibility page…
                      {{else}}
                        <lbh3-erection-eligibility
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          secondaryPage:from="scope.vm.secondaryPage"
                          title:to="scope.vm.title"
                          urlId:from="scope.vm.urlId"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "results"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/erection/results/">
                      {{#if isPending}}
                        Loading the erection results page…
                      {{else}}
                        <lbh3-erection-results
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          secondaryPage:from="scope.vm.secondaryPage"
                          title:to="scope.vm.title"
                          urlId:from="scope.vm.urlId"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
              {{/switch}}
            {{else}}
              <can-import from="~/components/erection/">
                {{#if isPending}}
                  Loading the erection page…
                {{else}}
                  <lbh3-erection
                    class="container pl-0 pr-0"
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    secondaryPage:from="scope.vm.secondaryPage"
                    title:to="scope.vm.title"
                    urlId:from="scope.vm.urlId"
                  />
                {{/if}}
              </can-import>
            {{/if}}
          {{else}}
            <can-import from="~/components/erections/">
              {{#if isPending}}
                Loading the erections page…
              {{else}}
                <lbh3-erections
                  description:to="scope.vm.description"
                  ogTitle:to="scope.vm.ogTitle"
                  title:to="scope.vm.title"
                />
              {{/if}}
            </can-import>
          {{/if}}
        {{/case}}
        {{#case "events"}}
          {{#if secondaryPage}}
            {{#switch secondaryPage}}
              {{#case "attendance"}}
                <lbh3-no-ssr>
                  <can-import from="~/components/run/attendance/">
                    {{#if isPending}}
                      Loading the run attendance sheet page…
                    {{else}}
                      <lbh3-run-attendance
                        day:from="scope.vm.day"
                        description:to="scope.vm.description"
                        month:from="scope.vm.month"
                        ogTitle:to="scope.vm.ogTitle"
                        title:to="scope.vm.title"
                        trailNumber:from="scope.vm.trailNumber"
                        year:from="scope.vm.year"
                      />
                    {{/if}}
                  </can-import>
                </lbh3-no-ssr>
              {{/case}}
              {{#case "edit"}}
                <lbh3-no-ssr>
                  {{#if urlId}}
                    <can-import from="~/components/special-event/edit/">
                      {{#if isPending}}
                        Loading the edit page…
                      {{else}}
                        <lbh3-special-event-edit
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                          urlId:from="scope.vm.urlId"
                          year:from="scope.vm.year"
                        />
                      {{/if}}
                    </can-import>
                  {{else}}
                    <can-import from="~/components/run/edit/">
                      {{#if isPending}}
                        Loading the edit run page…
                      {{else}}
                        <lbh3-run-edit
                          day:from="scope.vm.day"
                          description:to="scope.vm.description"
                          month:from="scope.vm.month"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                          trailNumber:from="scope.vm.trailNumber"
                          year:from="scope.vm.year"
                        />
                      {{/if}}
                    </can-import>
                  {{/if}}
                </lbh3-no-ssr>
              {{/case}}
              {{#case "founders"}}
                <can-import from="~/components/founders-2018/">
                  {{#if isPending}}
                    Loading…
                  {{else}}
                    <lbh3-founders-2018
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      title:to="scope.vm.title"
                    />
                  {{/if}}
                </can-import>
              {{/case}}
              {{#case "memorial-service"}}
                <can-import from="~/components/memorial-service/">
                  {{#if isPending}}
                    Loading…
                  {{else}}
                    <lbh3-memorial-service
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      title:to="scope.vm.title"
                    />
                  {{/if}}
                </can-import>
              {{/case}}
            {{/switch}}
          {{else}}
            {{#if trailNumber}}
              <can-import from="~/components/run/">
                {{#if isPending}}
                  Loading trail #{{scope.vm.trailNumber}}…
                {{else}}
                  <lbh3-run
                    day:from="scope.vm.day"
                    description:to="scope.vm.description"
                    month:from="scope.vm.month"
                    ogTitle:to="scope.vm.ogTitle"
                    title:to="scope.vm.title"
                    trailNumber:from="scope.vm.trailNumber"
                    year:from="scope.vm.year"
                  />
                {{/if}}
              </can-import>
            {{else}}
              {{#if urlId}}
                <can-import from="~/components/special-event/">
                  {{#if isPending}}
                    Loading…
                  {{else}}
                    <lbh3-special-event
                      class="container pl-0 pr-0"
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      title:to="scope.vm.title"
                      urlId:from="scope.vm.urlId"
                      year:from="scope.vm.year"
                    />
                  {{/if}}
                </can-import>
              {{else}}
                <can-import from="~/components/past-runs/">
                  {{#if isPending}}
                    Loading Past Runs…
                  {{else}}
                    <lbh3-past-runs
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      showHashit:bind="scope.vm.showHashit"
                      showNotes:bind="scope.vm.showNotes"
                      showOnOn:bind="scope.vm.showOnOn"
                      showScribe:bind="scope.vm.showScribe"
                      title:to="scope.vm.title"
                      year:from="scope.vm.year"
                    />
                  {{/if}}
                </can-import>
              {{/if}}
            {{/if}}
          {{/if}}
        {{/case}}
        {{#case "hareline"}}
          <can-import from="~/components/hareline/nav/">
            {{#if isPending}}
              Loading the Hareline navigation…
            {{else}}
              <lbh3-hareline-nav
                secondaryPage:from="secondaryPage"
              />
            {{/if}}
          </can-import>
          {{#switch secondaryPage}}
            {{#case "add"}}
              <lbh3-no-ssr>
                <can-import from="~/components/hareline/add/">
                  {{#if isPending}}
                    Loading the Hareline add page…
                  {{else}}
                    <lbh3-hareline-add
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      title:to="scope.vm.title"
                    />
                  {{/if}}
                </can-import>
              </lbh3-no-ssr>
            {{/case}}
            {{#case "edit"}}
              <lbh3-no-ssr>
                <can-import from="~/components/run/edit/">
                  {{#if isPending}}
                    Loading the edit run page…
                  {{else}}
                    <lbh3-run-edit
                      day:from="scope.vm.day"
                      description:to="scope.vm.description"
                      month:from="scope.vm.month"
                      ogTitle:to="scope.vm.ogTitle"
                      title:to="scope.vm.title"
                      trailNumber:from="scope.vm.trailNumber"
                      year:from="scope.vm.year"
                    />
                  {{/if}}
                </can-import>
              </lbh3-no-ssr>
            {{/case}}
            {{#case "guidelines"}}
              <can-import from="~/components/hareline/guidelines/">
                {{#if isPending}}
                  Loading haring guidelines…
                {{else}}
                  <lbh3-hareline-guidelines
                    class="container pl-0 pr-0"
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    title:to="scope.vm.title"
                  />
                {{/if}}
              </can-import>
            {{/case}}
            {{#default}}
              <can-import from="~/components/hareline/">
                {{#if isPending}}
                  Loading the Hareline…
                {{else}}
                  <lbh3-hareline
                    description:to="scope.vm.description"
                    ogTitle:to="scope.vm.ogTitle"
                    title:to="scope.vm.title"
                  />
                {{/if}}
              </can-import>
            {{/default}}
          {{/switch}}
        {{/case}}

        {{#case "hashers"}}
          {{#if id}}
            {{#if secondaryPage}}
              {{#switch secondaryPage}}
                {{#case "edit"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/hasher/edit/">
                      {{#if isPending}}
                        Loading the edit hasher page…
                      {{else}}
                        <lbh3-hasher-edit
                          description:to="scope.vm.description"
                          id:from="scope.vm.id"
                          ogTitle:to="scope.vm.ogTitle"
                          secondaryPage:from="secondaryPage"
                          title:to="scope.vm.title"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
              {{/switch}}
            {{else}}
              <can-import from="~/components/hasher/">
                {{#if isPending}}
                  Loading hasher page…
                {{else}}
                  <lbh3-hasher
                    description:to="scope.vm.description"
                    id:from="scope.vm.id"
                    ogTitle:to="scope.vm.ogTitle"
                    secondaryPage:from="secondaryPage"
                    skip:from="scope.vm.skip"
                    title:to="scope.vm.title"
                  />
                {{/if}}
              </can-import>
            {{/if}}
          {{else}}
            <can-import from="~/components/hashers/nav/">
              {{#if isPending}}
                Loading the Hashers navigation…
              {{else}}
                <lbh3-hashers-nav
                  secondaryPage:from="secondaryPage"
                />
              {{/if}}
            </can-import>
            {{#if secondaryPage}}
              {{#switch secondaryPage}}
                {{#case "add"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/hashers/add/">
                      {{#if isPending}}
                        Loading the add hasher page…
                      {{else}}
                        <lbh3-hashers-add
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "attendance-records"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/hashers/attendance-records/">
                      {{#if isPending}}
                        Loading the attendance records page…
                      {{else}}
                        <lbh3-hashers-attendance-records
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "early-warning"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/hashers/early-warning/">
                      {{#if isPending}}
                        Loading the early warning page…
                      {{else}}
                        <lbh3-hashers-early-warning
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
                {{#case "email"}}
                  <lbh3-no-ssr>
                    <can-import from="~/components/hashers/email/">
                      {{#if isPending}}
                        Loading the hashers email page…
                      {{else}}
                        <lbh3-hashers-email
                          description:to="scope.vm.description"
                          ogTitle:to="scope.vm.ogTitle"
                          title:to="scope.vm.title"
                        />
                      {{/if}}
                    </can-import>
                  </lbh3-no-ssr>
                {{/case}}
              {{/switch}}
            {{else}}
              {{#if id}}
              {{else}}
                <can-import from="~/components/hashers/">
                  {{#if isPending}}
                    Loading Hashers page…
                  {{else}}
                    <lbh3-hashers
                      description:to="scope.vm.description"
                      ogTitle:to="scope.vm.ogTitle"
                      searchNoHeadshot:bind="scope.vm.noHeadshot"
                      searchText:bind="scope.vm.search"
                      skip:from="scope.vm.skip"
                      title:to="scope.vm.title"
                    />
                  {{/if}}
                </can-import>
              {{/if}}
            {{/if}}
          {{/if}}
        {{/case}}
        {{#case "home"}}
          <can-import from="~/components/home/">
            {{#if isPending}}
              <div class="alert alert-info" role="alert">
                <strong>Fetching the next run…</strong>
              </div>
            {{else}}
              {{#if isRejected}}
                <div class="alert alert-danger" role="alert">
                  <strong>Fuck!</strong> The next run failed to load.
                  Check <a href="http://www.hash.org" target="_blank">hash.org</a> for the next run.
                </div>
              {{else}}
                <lbh3-home
                  class="container pl-0 pr-0"
                  description:to="scope.vm.description"
                  ogTitle:to="scope.vm.ogTitle"
                  title:to="scope.vm.title"
                />
              {{/if}}
            {{/if}}
          </can-import>
        {{/case}}
        {{#case "special-events"}}
          <can-import from="~/components/special-events/">
            {{#if isPending}}
              Loading special events page…
            {{else}}
              <lbh3-special-events
                description:to="scope.vm.description"
                ogTitle:to="scope.vm.ogTitle"
                title:to="scope.vm.title"
              />
            {{/if}}
          </can-import>
        {{/case}}
        {{#case "users"}}
          {{#if secondaryPage}}
            {{#switch secondaryPage}}
              {{#case "edit"}}
                <lbh3-no-ssr>
                  <can-import from="~/components/users/edit/">
                    {{#if isPending}}
                      Loading the edit user page…
                    {{else}}
                      <lbh3-user-edit
                        description:to="scope.vm.description"
                        id:from="scope.vm.id"
                        ogTitle:to="scope.vm.ogTitle"
                        title:to="scope.vm.title"
                      />
                    {{/if}}
                  </can-import>
                </lbh3-no-ssr>
              {{/case}}
            {{/switch}}
          {{else}}
            <can-import from="~/components/users/">
              {{#if isPending}}
                Loading users page…
              {{else}}
                <lbh3-users
                  description:to="scope.vm.description"
                  ogTitle:to="scope.vm.ogTitle"
                  secondaryPage:from="secondaryPage"
                  title:to="scope.vm.title"
                />
              {{/if}}
            </can-import>
          {{/if}}
        {{/case}}
      {{/switch}}
    </main>

    <lbh3-footer class="d-print-none" />

    {{#switch nodeEnv}}
      {{#case "production"}}
        <script src="{{joinBase 'steal.production.js'}}"></script>
      {{/case}}
      {{#default}}
        <script src="/node_modules/steal/steal.js" main></script>
      {{/default}}
    {{/switch}}
  </body>
</html>
