<footer class="container text-muted">
  <p class="float-sm-left">
    <a href="mailto:contact@lbh3.org">Email us</a> with any questions, comments, or concerns.
  </p>
  <p class="float-sm-right">
    {{#if session.user}}
      <button class="btn btn-link" type="button" on:click="session.destroy()">
        Sign out
      </button>
    {{else}}
      {{^eq platform.isNode true}}
        <a href="/auth/google">Sign in</a>
      {{/eq}}
    {{/if}}
  </p>
</footer>
