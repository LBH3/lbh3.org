{{#if templatePromise.isPending}}
  <div class="alert alert-info" role="alert">
    <strong>Fetching run…</strong>
  </div>
{{/if}}

{{#if templatePromise.isRejected}}
  <div class="alert alert-danger" role="alert">
    <strong>Uh oh!</strong> The run couldn’t be retrieved.
  </div>
{{/if}}

{{#if templatePromise.isResolved}}
  {{{template}}}
{{/if}}
